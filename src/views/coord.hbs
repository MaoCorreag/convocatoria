        <div class="card-body">
            <h1>Solicitudes</h1>
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">id</th>
                    <th scope="col">nombre</th>
                    <th scope="col">apellidos</th>
                    <th scope="col">c√©dula</th>
                    <th scope="col">celular</th>
                    <th scope="col">#</th>

                </tr>
                </thead>
                <tbody>
                {{#each empleados}}
